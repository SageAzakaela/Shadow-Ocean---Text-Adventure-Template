[node name="Syndicate" parent="ActionPanel/VBoxContainer/MemoryContainer/RememberMechanic" instance=ExtResource("9_0d0et")]

[node name="KiminiDalaz" parent="ActionPanel/VBoxContainer/MemoryContainer/RememberMechanic" instance=ExtResource("9_0d0et")]

